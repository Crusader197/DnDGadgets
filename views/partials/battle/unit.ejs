<div id="p1" class="pFrame m-3 p-3">
  <div class="row">  
    <div class="col-4 pr-0">
      <image class="img-thumbnail pIcon" src="images/user-generic.jpg">
    </div>
    <div class="col p-1">          
      <%
      	var i = x[0];
      %>      
      <%=party.players[i].name%>
      <div class="progress">
        <div class="progress-bar bg-danger" id="p1hp" style="width: <%=(party.players[i].currHp/party.players[i].maxHp) * 100%>%;">
        </div>
      </div>
      <%=party.players[i].currHp%>/<%=party.players[i].maxHp%>
    </div>
  </div>
</div>